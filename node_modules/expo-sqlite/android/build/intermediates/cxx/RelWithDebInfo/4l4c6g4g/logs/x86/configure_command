  -DANDROID_PLATFORM=android-24 \
  -DANDROID_ABI=x86 \
  -DCMAKE_ANDROID_ARCH_ABI=x86 \
  -DANDROID_NDK=/home/ubuntu/Android/Sdk/ndk/26.1.10909125 \
  -DCMAKE_ANDROID_NDK=/home/ubuntu/Android/Sdk/ndk/26.1.10909125 \
  -DCMAKE_TOOLCHAIN_FILE=/home/ubuntu/Android/Sdk/ndk/26.1.10909125/build/cmake/android.toolchain.cmake \
  -DCMAKE_MAKE_PROGRAM=/home/ubuntu/Android/Sdk/cmake/3.22.1/bin/ninja \
  -DCMAKE_LIBRARY_OUTPUT_DIRECTORY=/workspace/node_modules/expo-sqlite/android/build/intermediates/cxx/RelWithDebInfo/4l4c6g4g/obj/x86 \
  -DCMAKE_RUNTIME_OUTPUT_DIRECTORY=/workspace/node_modules/expo-sqlite/android/build/intermediates/cxx/RelWithDebInfo/4l4c6g4g/obj/x86 \
  -DCMAKE_BUILD_TYPE=RelWithDebInfo \
  -DCMAKE_FIND_ROOT_PATH=/workspace/node_modules/expo-sqlite/android/.cxx/RelWithDebInfo/4l4c6g4g/prefab/x86/prefab \
  -B/workspace/node_modules/expo-sqlite/android/.cxx/RelWithDebInfo/4l4c6g4g/x86 \
  -GNinja \
  -DANDROID_STL=c++_shared \
  -DSQLITE3_SRC_DIR=/workspace/node_modules/expo-sqlite/android/../vendor/sqlite3 \
  "-DSQLITE_BUILDFLAGS=-DSQLITE_ENABLE_BYTECODE_VTAB=1 -DSQLITE_TEMP_STORE=2 -DSQLITE_ENABLE_FTS4=1 -DSQLITE_ENABLE_FTS3_PARENTHESIS=1 -DSQLITE_ENABLE_FTS5=1" \
  -DUSE_SQLCIPHER=false \
  -DUSE_LIBSQL=false
